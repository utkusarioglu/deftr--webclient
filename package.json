{
  "name": "@deftr/webclient",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage --no-watchman a",
    "eject": "react-scripts eject",
    "create:doc": "$(yarn bin)/typedoc",
    "clean:doc": "rm -rf docs",
    "doc": "yarn clean:doc && yarn create:doc",
    "test:clear-cache": "jest --clearCache"
  },
  "dependencies": {
    "@giantmachines/redux-websocket": "^1.4.0",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-thunk": "^2.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "enzyme": "^3.11.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "4.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleNameMapper": {
      "^office-ui-fabric-react[/\\\\]lib(.*)": "office-ui-fabric-react/lib-commonjs$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!office-ui-fabric-react)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
